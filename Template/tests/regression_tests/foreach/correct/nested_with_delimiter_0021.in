{* file: nested_with_delimiter_0021.in *}
{var $foo = array( 0, 1, 3 ), $foo2 = array( '', 'foo' ), $bar = 0, $bar2 = 0, $num = 0, $num2 = 0, $i = 0, $j = 0}
\{0021
{foreach 1..1 as $blackhole}
    {$i = 0}
    {foreach $foo as $bar}
        {switch $bar}
        {case 1}
            {$i}:[switch]1[/switch]
        {/case}
        {case 2}
            {$i}:[switch]2[/switch]
        {/case}
        {default}
            {$i}:[switch]default({$bar})[/switch]
        {/default}
        {/switch}
        {$j = 0}
        {foreach $foo2 as $bar2}
            {switch $bar2}
            {case 1}
                {$i}:{$j}:[switch]1[/switch]
            {/case}
            {case 2}
                {$i}:{$j}:[switch]2[/switch]
            {/case}
            {default}
                {$i}:{$j}:[switch]default({$bar2})[/switch]
            {/default}
            {/switch}
            {delimiter}
                {$i}:{$j}:====delim====
            {/delimiter}
            {delimiter modulo 2}
                {$i}:{$j}:____delim____
            {/delimiter}
        {++$j}
        {/foreach}
    {++$i}
    {/foreach}
{/foreach}
\}
